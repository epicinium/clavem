{
    "name": "@epicinium/clavem",
    "version": "1.0.0-rc.0",
    "description": "An implementation of Ed25519 by SUPERCOP ref10.",
    "homepage": "https://github.com/epicinium/clavem",
    "bugs": {
        "url": "https://github.com/epicinium/clavem/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com"
    },
    "main": "releases/index.js",
    "types": "clavem.d.ts",
    "directories": {
        "lib": "releases",
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/epicinium/clavem.git"
    },
    "scripts": {
        "build": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run build:prod; else npm run build:dev; fi",
        "build:dev": "NODE_ENV=development npm run compile",
        "build:prod": "NODE_ENV=production npm run compile",
        "compile": "npm run compile:babel",
        "compile:babel": "npx babel sources --out-dir releases --delete-dir-on-start",
        "test": "npm run test:eslint",
        "test:eslint": "npx eslint ."
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/plugin-transform-async-to-generator": "^7.1.0",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/preset-env": "^7.1.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.14.0"
    },
    "engines": {
        "node": "^10.13.0",
        "npm": "^6.4.1"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    }
}
